#!/bin/bash

export SEED=${RANDOM}

for n in 1; do
  for s in 1 10 100 1000; do
    echo ./bench deser --number $n --size $s
    ./bench deser --number $n --size $s > output/deser-$n-$s.txt
    echo ./bench ser --number $n --size $s
    ./bench ser --number $n --size $s > output/ser-$n-$s.txt
  done
done

